{
    "name": "timurmametev/finance",
    "type": "project",
    "authors": [
        {
            "name": "timurmametev"
        }
    ],
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "php-di/php-di": "^6.0",
        "slim/psr7": "^1.5",
        "slim/slim": "^4.10",
        "symfony/console": "^6.0"
    },
    "require-dev": {
        "overtrue/phplint": "^5.0",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.5",
        "vimeo/psalm": "^4.22"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "app": "php bin/app.php --ansi",
        "cs-check": "phpcs",
        "lint": "phplint",
        "psalm": "psalm"
    }
}
